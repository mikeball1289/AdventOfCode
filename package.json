{
  "name": "adventofcode2020",
  "version": "1.0.0",
  "description": "Solutions for the Advent of Code 2020 (https://adventofcode.com/2020)",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf bin",
    "test": "jest",
    "test:coverage": "jest --collectCoverage lib",
    "cloc": "cloc days lib --by-file",
    "cloc:app": "cloc days lib --by-file --not-match-f=\"\\.test\\.ts$\"",
    "cloc:tests": "cloc days lib --by-file --match-f=\"\\.test\\.ts$\"",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeball1289/AdventOfCode2020.git"
  },
  "author": "Mike Ball",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikeball1289/AdventOfCode2020/issues"
  },
  "homepage": "https://github.com/mikeball1289/AdventOfCode2020#readme",
  "dependencies": {
    "@types/node": "^14.14.10",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "cloc": "^2.7.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3"
  }
}
